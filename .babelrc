{
    "presets": [
        ["@babel/preset-env"],
        ["@babel/preset-react"],
        [
            "env",
            {
                "targets": {
                    "browsers": [
                        "last 1 Chrome version",
                        "last 1 Firefox version",
                        "last 1 Safari version",
                        "last 1 Opera version",
                        "last 1 Edge version"
                    ]
                }
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "transform-object-rest-spread"
    ]
}
